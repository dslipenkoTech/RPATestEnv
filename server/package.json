{
  "name": "web-tech-test-suite",
  "version": "2.0.0",
  "type": "module",
  "description": "An RPA testing suite for the major websites of Moody Bible Institute",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon",
    "dev": "npx tsx src/app.ts",
    "prod": "npx cypress open --env ENV_TYPE=production",
    "pre": "npx cypress open --env ENV_TYPE=pre-production",
    "int": "npx cypress open --env ENV_TYPE=integration",
    "cy:dash": "npx tsx run-cypress.ts --record --config screenshotOnRunFailure=false --browser electron",
    "cy:run": "cypress run",
    "custom:session": "cypress run --spec cypress/e2e/generalFunctions/edu/general-detail-page.dxp --env ENV_TYPE=integration",
    "cy:open": "cypress open",
    "cy:run:front": "cypress run --record --spec cypress\\e2e\\Tickets\\Frontend",
    "cy:pub": "cypress run --record --spec cypress\\e2e\\Tickets\\Frontend\\Publishers",
    "cy:bible": "cypress run --record --spec cypress\\e2e\\Tickets\\Frontend\\Bible",
    "cy:edu": "cypress run --record --spec cypress\\e2e\\Tickets\\Frontend\\Education",
    "cy:radio": "cypress run --record --spec cypress\\e2e\\Tickets\\Frontend\\Radio",
    "cy:spanradio": "cypress run --record --spec cypress\\e2e\\Tickets\\Frontend\\SpanRadio",
    "cy:titw": "cypress run --record --spec cypress\\e2e\\Tickets\\Frontend\\TITW"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbi-webtech/WebTechTestSuite.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mbi-webtech/WebTechTestSuite/issues"
  },
  "homepage": "https://github.com/mbi-webtech/WebTechTestSuite#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/he": "^1.2.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.x",
    "@types/react-redux": "^7.1.34",
    "@types/redux": "^3.6.31",
    "cypress": "^15.2.0",
    "cypress-if": "^1.13.2",
    "cypress-wait-until": "^3.0.2",
    "electron": "^30.0.6",
    "he": "^1.2.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.9",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^4.9.5",
    "webpack": "^5.98.0"
  },
  "dependencies": {
    "@cypress/grep": "^4.1.0",
    "axios": "^1.8.4",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "cypress-tags": "^1.2.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mongoose": "^8.9.4",
    "motion": "^12.23.12",
    "pusher": "^5.2.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  }
}
